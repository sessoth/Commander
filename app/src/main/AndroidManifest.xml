<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.commander">

    <application
        android:name=".Commander"
        android:allowBackup="true"
        android:icon="@drawable/commander_legends_symbol"
        android:label="@string/app_name"
        android:roundIcon="@drawable/commander_legends_symbol"
        android:supportsRtl="true"
        android:exported="true"
        android:theme="@style/Theme.Commander">
        <activity android:name=".MainActivity"
            android:exported="true"
            android:forceDarkAllowed="true"
            android:label="@string/listviewTitle" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>